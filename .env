# .env file in the root directory of your project

# Public environment variables (accessible on both client-side and server-side)
NEXT_PUBLIC_API_URL=https://api.example.com  # Replace with your actual API URL for backend interactions
NEXT_PUBLIC_ANALYTICS_ID=your_analytics_id  # Replace with your actual analytics service ID

# Server-only environment variables (not accessible on the client-side)
DATABASE_URL=your_database_url  # Replace with your actual database URL for server-side operations
SECRET_KEY=your_secret_key  # Replace with your secret key, used for cryptographic or secure server-side functions
